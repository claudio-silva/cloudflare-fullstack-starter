    "kysely": "^0.28.8",
    "kysely-d1": "^0.4.0",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "react": "19.0.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.3",
    "react-router-dom": "^6.28.0",
