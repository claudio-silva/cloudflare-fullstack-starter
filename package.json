    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "9.27.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vite": "^6.4.1",
    "wrangler": "^4.53.0"
  }
}
